	{
		var url = "/" + links[i].getAttribute("href");
		if (window.location.href.match(url + "\x24") == url)
		{
			var cls = links[i].getAttribute("class");
			links[i].setAttribute("class", cls ? cls + " active" : "active");
			break;
		}
	}

	var placeholder = document.getElementById("group-view-mode-placeholder");
	if (placeholder && viewModeTemplate)
		if (window.history.pushState)
			placeholder.innerHTML = viewModeTemplate.replace('__URL__', location.pathname);
}
</script>

